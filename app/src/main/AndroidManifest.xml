<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sacramentmeetingplanner">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SacramentMeetingPlanner">
        <activity
            android:name=".ViewSavedAgenda"
            android:label="@string/title_activity_view_saved_agenda"
            android:theme="@style/Theme.SacramentMeetingPlanner.NoActionBar"></activity>
        <activity android:name=".EditActivity" />
        <activity android:name=".MeetingPlan" />
        <activity
            android:name=".Template"
            android:label="@string/title_activity_template"
            android:theme="@style/Theme.SacramentMeetingPlanner.NoActionBar" />
        <activity android:name=".Login" />
        <activity android:name=".Register" />
        <activity android:name=".Agenda" />
        <activity android:name=".Announcements" />
        <activity android:name=".SacramentProgram" />
        <activity android:name=".StakeBusiness" />
        <activity android:name=".WardBusiness" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>